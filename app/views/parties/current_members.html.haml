- page_title = title("#{@party.name} - #{t('.title', current: t('current'), members: t('members'))}")

%h1= page_title
= render partial: 'shared/id_a_to_z', locals: { route_symbol: :party_members_current_a_z_letter_path, id: @party.graph_id}
= render partial: 'pugin/cards/person-list', locals: { people: @people, letter: params[:letter] }

.block
  %p= link_to(t('past_and_present_members'), party_members_a_z_letter_path(@party.graph_id, 'a'))

