%li.list__item
  - if incumbency.house.nil?
    %h2= link_to(incumbency.seat.house.name, house_path(incumbency.seat.house.graph_id))
  - else
    %h2= link_to(incumbency.house.name, house_path(incumbency.house.graph_id))

  %p
    = "#{l(incumbency.start_date, format: :default)}" unless incumbency.start_date.nil?
    = incumbency.current? ? t('to_present') : " #{t('to')} #{l(incumbency.end_date, format: :default)}"
